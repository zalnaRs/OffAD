{
  "name": "OffAD",
  "version": "1.0",
  "description": "Easy, secure, fast way to block ads and trackers, Take back your privacy!",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["./src/blocked_domains.js", "./src/background.js"]
  },
  "browser_action": {
    "default_popup": "./src/popup.html"
  },
  "manifest_version": 2
}
